{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "ESnext",
      "ES2017",
      "ES2015",
      "DOM"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "types": [
      "node",
      "body-parser"
    ],
    "strict": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts"
  ]
}
